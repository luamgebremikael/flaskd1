{% extends 'base.jinja' %}

{%block content %}
    <h2>Login page</h2>
    <form action="" method="post">
    {{ form.hidden_tag() }}
    <p>
    {{form.email.label}}<br>
    {{form.email(size=32)  }}
    </p>
    <p>
    {{form.password.label}}<br>
    {{form.password(size=32) }}
    </p>
    <p>
    {{form.submit(class='btn btn-primary') }}
    
    </p>
    
    
    
    </form>
{% endblock%}
